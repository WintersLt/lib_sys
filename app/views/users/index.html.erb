<table>
  <tr>
        <th>User Name</th>
        <th>email</th>
              </tr>

              <% @users.each do |user| %>

			<% if logged_in_as_admin? %>
                      	    <tr>
                              <td><%= user.name %></td>
                              <td><%= user.email %></td>
				<td><%= link_to 'Delete' , user_path(user), method: :delete, data: {confirm: 'Are you sure?'} %></td>
                            </tr>
			<% end %>

                  <% end %>
      </table>

<div>
	<%= link_to "Back" , user_path(current_user) %>
</div>








