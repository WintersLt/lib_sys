<% provide(:title, @current_user.name) %>
<table>
  <tr>
        <th>User Name</th>
        <th>email</th>
              </tr>
              	<% @users.each do |user| %>
				<% if user.is_admin %>
                      	    <tr>
                              <td><%= user.name %></td>
                              <td><%= user.email %></td>
				<td><%= link_to 'Delete user' , user_path(user), method: :delete, data: {confirm: 'Are you sure?'} %></td>
                            </tr>
			<% end %>

                  <% end %>
      </table>

<div>
	<%= link_to "Back" , user_path(current_user) %>
</div>








