<% provide(:title, @current_user.name) %>
<table>
  <tr>
        <th>User Name</th>
        <th>email</th>
              </tr>

              <% @users.each do |user| %>
		
			
			<% if user.is_lib_member %>
                      	    <tr>
                              <td><%= user.name %></td>
                              <td><%= user.email %></td>
				<td><%= link_to 'Delete' , delete_lib_members_path(user.id), method: :delete, data: {confirm: 'Are you sure?'} %></td>
                            </tr>
			<% end %>

                  <% end %>
      </table>

<div>
	<%= link_to "Back" , user_path(current_user) %>
</div>








